void loop() {
  currentMillis = millis();
  micOutput = findPTPAmp();
  soundArray[i] = micOutput;
  ambiantSound = soundArray[0] + soundArray[1] + soundArray[2] + soundArray[3] + soundArray[4] + soundArray[5] / 6;
  Serial.println(micOutput);
  if (!running && currentMillis - startMillis >= 3000) {
    Serial.println("determine l'etat");

    if (success) {
      state--;
      success = false;
      Serial.println("SUCCESS");
    } else if (micOutput >= 80 && state < 3) {
      state = 3;
    } else if (micOutput >= 30 && state < 2) {
      state = 2;
    } else if (state <= 1) {
      state = 1;
    }
    running = true;
  }

  if (running && i < 6) {
    switch (state) {
      case 1:

        Serial.println("Peacefull");
        if (currentMillis - startMillis >= 1000) {
          startMillis = millis();
          running = false;
        }
        break;
      case 2:
        Serial.println("va vibrer 2");
        vibration();
        break;

      case 3:
        Serial.println("va vibrer 3");
        vibration();
        break;
    }

    checkSuccess();

    if (i == 5) {
      i = 0;
      running = false;
      successCount = 0;
      for (int j = 0; j < 6; j++) {
        beatCheck[j] = false;
      }
    }
  }

  DrawBlob();
}



